{
  "name": "multi-agent-architecture-system",
  "version": "1.0.0",
  "description": "Production-ready MCP server orchestrating 40+ specialized AI agents for automated software architecture design",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,json,md}\"",
    "test": "vitest",
    "test:unit": "vitest run --reporter=verbose tests/unit",
    "test:integration": "vitest run --reporter=verbose tests/integration",
    "test:e2e": "vitest run --reporter=verbose tests/e2e",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest --watch",
    "server:debug": "DEBUG=* npm run dev",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "postbuild": "npm run type-check"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "ai-agents",
    "software-architecture",
    "architecture-design",
    "multi-agent-system",
    "claude",
    "anthropic",
    "typescript",
    "automation"
  ],
  "author": "chipster6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chipster6/multi-agent-architecture-system.git"
  },
  "bugs": {
    "url": "https://github.com/chipster6/multi-agent-architecture-system/issues"
  },
  "homepage": "https://github.com/chipster6/multi-agent-architecture-system#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@anthropic-ai/sdk": "^0.27.0",
    "zod": "^3.22.4",
    "sqlite3": "^5.1.6",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}